{
  "name": "pant-react",
  "version": "0.1.0",
  "description": "Mobile UI Components built on React, Ported from Vant",
  "keywords": [
    "react",
    "ui",
    "vant"
  ],
  "author": "Gary Wang",
  "license": "MIT",
  "homepage": "https://github.com/webyom/pant-react",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webyom/pant-react.git"
  },
  "bugs": {
    "url": "https://github.com/webyom/pant-react/issues"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build:lib": "tsc -d -m commonjs --outDir lib --sourceMap false --inlineSources false",
    "build:es": "tsc -d -m esNext --outDir es --sourceMap false --inlineSources false",
    "build:clean": "rm -rf lib/demos/ es/demos/ lib/*/demo/ es/*/demo/",
    "build": "rm -rf lib es && npm run build:lib && npm run build:es && npm run scss && npm run build:clean",
    "start": "webpack-dev-server",
    "deploy": "NODE_ENV=production webpack && gh-pages -t -d docs",
    "scss": "gulp scss && node-sass src --output lib && node-sass src --output es",
    "tsc-check": "tsc --outDir .tsc-check/",
    "eslint": "eslint --ext ts,tsx src",
    "eslint:fix": "eslint --ext ts,tsx src --fix",
    "stylelint": "stylelint src/**/*.scss",
    "stylelint:fix": "stylelint src/**/*.scss --fix",
    "release": "standard-version"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.12",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.1.2",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "file-loader": "^5.1.0",
    "gh-pages": "^2.2.0",
    "gulp": "^4.0.2",
    "hammer-touchemulator": "0.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.0",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.13.1",
    "prettier": "^2.2.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "require-all": "^2.0.0",
    "sass-loader": "^8.0.2",
    "standard-version": "^9.0.0",
    "style-loader": "^1.1.3",
    "stylelint": "^12.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-scss": "^3.13.0",
    "ts-jest": "^26.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^4.2.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {
    "closest": "0.0.1",
    "clsx": "^1.1.0"
  }
}
